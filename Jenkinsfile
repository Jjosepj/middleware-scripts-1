pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello'
                sleep 1
            }
        }
        stage('build') {
            steps {
                echo 'Hello Build'
                sleep 1
            }
        }
        stage('test') {
            steps {
                echo 'Hello Test'
                sleep 1
            }
        }
        stage('deploy') {
            steps {
                echo 'Hello Deploy'
                sleep 1
            }
        }
    }
}
